{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "db:up": "docker compose -f supabase/docker-compose.yml up -d",
    "db:down": "docker compose -f supabase/docker-compose.yml down",
    "db:logs": "docker compose -f supabase/docker-compose.yml logs -f --tail=100",
    "db:psql": "docker compose -f supabase/docker-compose.yml exec -T db psql -U postgres -d postgres",
    "lint": "eslint .",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "qa:fast": "bash scripts/qa/fast-regression.sh",
    "qa:investigate": "tsx scripts/qa/investigate-last-run.ts",
    "qa:repair-last-run": "tsx scripts/qa/repair-last-run.ts",
    "qa:verify": "pnpm qa:fast && pnpm lint",
    "test:e2b": "npx tsx lib/e2b/__tests__/sandbox.test.ts",
    "template:build:dev": "npx tsx lib/e2b/templates/build.ts",
    "template:build:prod": "npx tsx lib/e2b/templates/build.ts",
    "template:build": "npx tsx lib/e2b/templates/build.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:report": "playwright show-report",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "playwright:install": "playwright install --with-deps",
    "sandbox": "tsx scripts/create-sandbox.ts",
    "presubmit": "pnpm lint && pnpm exec tsc --noEmit && pnpm test"
  },
